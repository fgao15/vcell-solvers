<?xml version="1.0">
<optProblemDescription>
   <parameterDescription>
       <parameter name="init" low="0.1" high="1.0" init="0.5" scale="1"/>
       <parameter name="koff" low="0.2" high="1.0" init="0.5" scale="1"/>
   </parameterDescription>
   <constraintDescription>
      <constraint constraintType="linearInequality">init - koff</constraint>
   </constraintDescription>
   <objectiveFunction type="explicit">
      init + koff
   </objectiveFunction>
</optProblemDescription>
